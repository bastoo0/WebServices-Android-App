<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation"
    app:startDestination="@id/menuFragment">
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.polytech.applicationws.MenuFragment"
        android:label="MenuFragment" >
        <action
            android:id="@+id/action_menuFragment_to_actorsFragment"
            app:destination="@id/actorsFragment" />
    </fragment>
    <fragment
        android:id="@+id/actorsFragment"
        android:name="com.polytech.applicationws.ActorsFragment"
        android:label="ActorsFragment" >
        <action
            android:id="@+id/action_actorsFragment_to_addActorFragment"
            app:destination="@id/addActorFragment" />
    </fragment>
    <fragment
        android:id="@+id/addActorFragment"
        android:name="com.polytech.applicationws.AddActorFragment"
        android:label="AddActorFragment" />
</navigation>